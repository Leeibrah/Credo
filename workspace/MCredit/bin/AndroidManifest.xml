<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.credit" >

    <uses-sdk android:minSdkVersion="8" />
    
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        >
        
        <uses-library android:name="com.google.android.maps" />  
        
        <activity
            android:name="com.land.SplashScreen"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.land.MainVoucher"></activity>
        <activity android:name="com.land.Login"></activity>
        <activity android:name=".com.location.CurrentLocation"></activity>
        <activity android:name=".Sales"></activity>
        <activity android:name=".Sold"></activity>
        <activity android:name=".Support"></activity>
        <activity android:name=".PhoneCall"></activity>
        <activity android:name=".MapLocation"></activity>
        <activity android:name=".Summary"></activity>
        <activity android:name=".xxx"></activity>
    </application>

</manifest>